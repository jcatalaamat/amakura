# Hot Updater Configuration
# Copy this to .env.hotupdater and fill in your Supabase credentials

# Supabase URL for hot updater storage
HOT_UPDATER_SUPABASE_URL=https://your-project.supabase.co

# Supabase anon key
HOT_UPDATER_SUPABASE_ANON_KEY=your-anon-key

# Supabase storage bucket name for update bundles
HOT_UPDATER_SUPABASE_BUCKET_NAME=hot-updates
