{
  "name": "@take-out/postgres",
  "version": "0.1.15",
  "sideEffects": false,
  "source": "src/index.ts",
  "main": "dist/cjs",
  "module": "dist/esm",
  "type": "module",
  "types": "./src/index.ts",
  "bin": {
    "postgres": "./cli.cjs"
  },
  "files": [
    "src",
    "types",
    "dist",
    "cli.cjs"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tamagui-build --skip-native --skip-types",
    "watch": "tamagui-build --skip-native --skip-types --watch",
    "typecheck": "tko run typecheck",
    "lint": "oxlint src",
    "lint:fix": "oxfmt src && oxlint --fix --fix-suggestions src",
    "clean": "tamagui-build clean",
    "clean:build": "tamagui-build clean:build"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./src/index.ts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs",
      "default": "./dist/esm/index.mjs"
    },
    "./*": {
      "types": "./src/*.ts",
      "import": "./dist/esm/*.mjs",
      "require": "./dist/cjs/*.cjs"
    }
  },
  "dependencies": {
    "@take-out/helpers": "workspace:*",
    "async-retry": "^1.3.3",
    "citty": "^0.1.6"
  },
  "peerDependencies": {
    "drizzle-orm": "*",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@tamagui/build": "2.0.0-rc.11",
    "@types/async-retry": "^1.4.8",
    "@types/node": "24.0.3",
    "drizzle-orm": "^0.40.0",
    "oxfmt": "^0.16.0",
    "oxlint": "^1.31.0"
  }
}
