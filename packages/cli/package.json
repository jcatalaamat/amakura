{
  "name": "@take-out/cli",
  "version": "0.1.15",
  "description": "CLI tools for Takeout starter kit - interactive onboarding and project setup",
  "type": "module",
  "sideEffects": false,
  "source": "src/index.ts",
  "main": "dist/cjs",
  "module": "dist/esm",
  "types": "./src/index.ts",
  "bin": {
    "takeout": "./src/cli.ts",
    "tko": "./src/cli.ts"
  },
  "files": [
    "src",
    "types",
    "dist",
    "scripts",
    "cli.mjs"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "bun": "./src/index.ts",
      "types": "./src/index.ts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs"
    },
    "./utils/*": {
      "types": "./src/utils/*.ts",
      "import": "./dist/esm/utils/*.mjs",
      "require": "./dist/cjs/utils/*.cjs"
    },
    "./constants/*": {
      "types": "./src/constants/*.ts",
      "import": "./dist/esm/constants/*.mjs",
      "require": "./dist/cjs/constants/*.cjs"
    },
    "./withOpSqliteStatic": "./src/withOpSqliteStatic.cjs"
  },
  "scripts": {
    "build": "tamagui-build --skip-native --skip-types",
    "watch": "tamagui-build --skip-native --skip-types --watch",
    "typecheck": "tko run typecheck",
    "lint": "oxlint src",
    "lint:fix": "oxfmt src && oxlint --fix --fix-suggestions src",
    "clean": "tamagui-build clean",
    "clean:build": "tamagui-build clean:build",
    "dev": "bun --watch src/cli.ts"
  },
  "dependencies": {
    "@clack/prompts": "^0.8.2",
    "@take-out/docs": "workspace:*",
    "@take-out/scripts": "workspace:*",
    "citty": "^0.1.6",
    "picocolors": "^1.1.1"
  },
  "peerDependencies": {
    "@expo/config-plugins": "*"
  },
  "peerDependenciesMeta": {
    "@expo/config-plugins": {
      "optional": true
    }
  },
  "devDependencies": {
    "@tamagui/build": "2.0.0-rc.11",
    "@types/node": "24.0.3",
    "bun-types": "latest",
    "oxfmt": "^0.16.0",
    "oxlint": "^1.31.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
