{
  "name": "@take-out/helpers",
  "version": "0.1.15",
  "sideEffects": false,
  "type": "module",
  "source": "src/index.ts",
  "main": "dist/cjs",
  "module": "dist/esm",
  "files": [
    "src",
    "types",
    "dist"
  ],
  "scripts": {
    "build": "tamagui-build --skip-types",
    "watch": "tamagui-build --skip-types --watch",
    "typecheck": "tko run typecheck",
    "lint": "oxlint src",
    "lint:fix": "oxfmt src && oxlint --fix --fix-suggestions src",
    "clean": "tamagui-build clean",
    "clean:build": "tamagui-build clean:build"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "react-native": {
        "import": "./dist/esm/index.native.js",
        "require": "./dist/cjs/index.native.js"
      },
      "types": "./src/index.ts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs",
      "default": "./dist/esm/index.mjs"
    },
    "./server-only": {
      "react-native": {
        "import": "./dist/esm/server-only.native.js",
        "require": "./dist/cjs/server-only.native.js"
      },
      "types": "./src/server-only.ts",
      "import": "./dist/esm/server-only.mjs",
      "require": "./dist/cjs/server-only.cjs",
      "default": "./dist/esm/server-only.mjs"
    },
    "./client-only": {
      "react-native": {
        "import": "./dist/esm/client-only.native.js",
        "require": "./dist/cjs/client-only.native.js"
      },
      "types": "./src/client-only.ts",
      "import": "./dist/esm/client-only.mjs",
      "require": "./dist/cjs/client-only.cjs",
      "default": "./dist/esm/client-only.mjs"
    }
  },
  "dependencies": {
    "@tamagui/constants": "2.0.0-rc.11",
    "dequal": "^2.0.3"
  },
  "peerDependencies": {
    "expo-clipboard": "*",
    "react": "*",
    "react-native": "*"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-native": {
      "optional": true
    },
    "expo-clipboard": {
      "optional": true
    },
    "@rocicorp/zero": {
      "optional": true
    }
  },
  "devDependencies": {
    "@tamagui/build": "2.0.0-rc.11",
    "@types/node": "24.0.3",
    "@types/react": "^19.0.8",
    "oxfmt": "^0.16.0",
    "oxlint": "^1.31.0",
    "react": "*",
    "react-native": "*"
  },
  "publishConfig": {
    "access": "public"
  }
}
