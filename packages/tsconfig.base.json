{
  "compilerOptions": {
    "rootDir": ".",
    "incremental": false,
    // "paths": {
    //   "@take-out/better-auth-utils": ["./better-auth-utils/src/index.ts"],
    //   "@take-out/helpers": ["./helpers/src/index.ts"],
    //   "@take-out/hooks": ["./hooks/src/index.ts"],
    //   "@take-out/postgres": ["./postgres/src/index.ts"],
    //   "@take-out/scripts": ["./scripts/src/index.ts"],
    //   "@take-out/cli": ["./takeout/src/index.ts"]
    // },
    "outDir": "dist",
    "importHelpers": true,
    "allowJs": false,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "declaration": true,
    "downlevelIteration": true,
    "strict": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "inlineSourceMap": true,
    "preserveSymlinks": true,
    "jsx": "react-jsx",
    "module": "Preserve",
    "moduleResolution": "Bundler",
    "noEmit": false,
    "noEmitOnError": false,
    "noImplicitReturns": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "preserveConstEnums": true,
    "removeComments": false,
    "skipLibCheck": true,
    "sourceMap": false,
    "noUncheckedIndexedAccess": true,
    "strictNullChecks": true,
    "target": "es2020",
    "types": ["vite/client", "node", "react", "bun"],
    "lib": ["dom", "esnext"]
  },
  "typeAcquisition": {
    "enable": true
  }
}
