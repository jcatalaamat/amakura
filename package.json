{
  "name": "takeout",
  "version": "0.0.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "files": [],
  "engines": {
    "node": "24.3.0",
    "npm": "10.8.3"
  },
  "packageManager": "bun@1.2.22",
  "workspaces": [
    "./packages/*"
  ],
  "scripts": {
    "preinstall": "bun ./scripts/preinstall.ts",
    "postinstall": "bun run ./scripts/postinstall.ts",
    "android:prebuild": "bun native:prebuild --platform android --no-install",
    "android": "one run:android",
    "aws": "tko aws",
    "backend:clean": "docker compose down --volumes --remove-orphans --rmi local; rm -rf ./.docker/*",
    "backend": "tko db build && docker compose up --remove-orphans",
    "build": "tko run build",
    "check:all": "tko check --all",
    "check": "tko check",
    "ops": "CI=true NODE_ENV=production bun tko ops",
    "clean": "tko run clean",
    "db:add-migration": "tko db add-migration",
    "db:migrate": "tko db migrate",
    "db": "tko db",
    "dev": "tko run-all --flags=last watch one:dev",
    "eas": "tko eas",
    "env:dev": "NODE_ENV=development dotenvx run -f .env .env.development --",
    "env:pull": "tko run env-pull",
    "env:update": "tko run env-update",
    "format:check": "oxfmt --check",
    "format": "oxfmt",
    "hot-update": "tko hot-update",
    "icon:add": "bun tko icon add-phosphor",
    "ios:prebuild": "bun native:prebuild --platform ios --no-install",
    "ios": "one run:ios",
    "lint:fix": "oxfmt && oxlint --import-plugin --type-aware --fix --fix-suggestions",
    "lint": "oxlint --import-plugin --type-aware",
    "native:prebuild": "one prebuild",
    "onboard": "tko onboard",
    "one:dev": "bun ./scripts/one-dev.ts",
    "repl": "tko run repl",
    "run:dev": "bun env:dev bun",
    "run:prod": "NODE_ENV=production bun tko run exec-with-env production bun",
    "test:integration": "bun run env:dev bun --cwd=./src/test playwright test",
    "test:unit": "bun run:dev vitest run -c ./src/test/vitest.config.ts",
    "test": "tko run test:unit test:integration",
    "up": "tko up",
    "watch:types": "tko run typecheck --watch",
    "watch:zero:generate": "bun zero:generate --watch",
    "watch": "tko run watch watch:types watch:zero:generate",
    "web": "tko web",
    "zero:generate": "on-zero generate --after 'oxfmt src/data/generated'",
    "zero": "tko zero"
  },
  "overrides": {
    "@expo/fingerprint": "0.15.3"
  },
  "dependencies": {
    "@aws-sdk/client-rekognition": "^3.975.0",
    "@aws-sdk/client-s3": "^3.709.0",
    "@aws-sdk/lib-storage": "^3.709.0",
    "@better-auth/expo": "^1.3.32",
    "@better-fetch/fetch": "^1.1.18",
    "@docsearch/css": "3.8.3",
    "@docsearch/react": "3.8.3",
    "@gorhom/bottom-sheet": "^5.2.6",
    "@hot-updater/bare": "0.25.8",
    "@hot-updater/react-native": "^0.25.8",
    "@hot-updater/supabase": "0.25.8",
    "@legendapp/list": "^2.0.18",
    "@op-engineering/op-sqlite": "^15.0.7",
    "@react-native-masked-view/masked-view": "^0.3.2",
    "@react-native-menu/menu": "^2.0.0",
    "@react-navigation/bottom-tabs": "^7.10.1",
    "@resvg/resvg-js": "^2.6.2",
    "@rocicorp/zero": "0.25.12",
    "@take-out/better-auth-utils": "workspace:*",
    "@take-out/helpers": "workspace:*",
    "@take-out/native-hot-update": "workspace:*",
    "@take-out/postgres": "workspace:*",
    "@tamagui/config": "2.0.0-rc.11",
    "@tamagui/constants": "2.0.0-rc.11",
    "@tamagui/helpers-icon": "2.0.0-rc.11",
    "@tamagui/linear-gradient": "2.0.0-rc.11",
    "@tamagui/menu": "2.0.0-rc.11",
    "@tamagui/native": "2.0.0-rc.11",
    "@tamagui/themes": "2.0.0-rc.11",
    "@tamagui/toast": "2.0.0-rc.11",
    "@tamagui/vite-plugin": "2.0.0-rc.11",
    "@types/jsonwebtoken": "^9.0.10",
    "@vxrn/color-scheme": "1.6.16",
    "@vxrn/mdx": "1.6.16",
    "better-auth": "^1.3.32",
    "burnt": "^0.12.2",
    "drizzle-orm": "^0.40.0",
    "expo": "^54.0.22",
    "expo-blur": "~15.0.7",
    "expo-build-properties": "~1.0.9",
    "expo-clipboard": "~8.0.7",
    "expo-crypto": "^15.0.7",
    "expo-dev-client": "~6.0.16",
    "expo-device": "~8.0.9",
    "expo-font": "~14.0.9",
    "expo-glass-effect": "~0.1.6",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.10",
    "expo-image-picker": "~17.0.8",
    "expo-linear-gradient": "~15.0.7",
    "expo-media-library": "~18.2.0",
    "expo-notifications": "~0.32.12",
    "expo-plugin-ios-static-libraries": "^1.0.4",
    "expo-splash-screen": "^31.0.10",
    "expo-web-browser": "^15.0.9",
    "hot-updater": "^0.25.8",
    "jsonwebtoken": "^9.0.2",
    "motion": "^12.29.0",
    "on-zero": "workspace:*",
    "one": "1.6.16",
    "pg": "^8.16.3",
    "posthog-js": "^1.252.0",
    "posthog-node": "^5.5.1",
    "posthog-react-native": "^4.1.0",
    "postmark": "^4.0.5",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-bottom-tabs": "^0.10.0",
    "react-native-fast-squircle": "^1.0.14",
    "react-native-gesture-handler": "~2.30.0",
    "react-native-ios-context-menu": "^3.2.1",
    "react-native-ios-utilities": "^5.2.0",
    "react-native-keyboard-controller": "^1.20.2",
    "react-native-mmkv": "^3.3.0",
    "react-native-permissions": "^5.4.2",
    "react-native-reanimated": "^4.2.1",
    "react-native-safe-area-context": "~5.6.2",
    "react-native-screens": "^4.19.0",
    "react-native-svg": "15.12.1",
    "react-native-teleport": "^0.5.6",
    "react-native-worklets": "^0.7.2",
    "satori": "^0.19.1",
    "tamagui": "2.0.0-rc.11",
    "zeego": "^3.0.6"
  },
  "devDependencies": {
    "@aws-sdk/client-ecr": "^3.859.0",
    "@clack/prompts": "^0.11.0",
    "@dotenvx/dotenvx": "^1.44.1",
    "@playwright/test": "^1.55.0",
    "@take-out/cli": "workspace:*",
    "@take-out/scripts": "workspace:*",
    "@types/aws-lambda": "8.10.147",
    "@types/bun": "^1.2.16",
    "@types/node": "24.0.3",
    "@types/pg": "^8.11.10",
    "@types/react-dom": "~19.1.7",
    "@types/react": "~19.1.10",
    "@types/subset-font": "^2",
    "@typescript/native-preview": "7.0.0-dev.20251211.1",
    "chrome-devtools-mcp": "^0.16.0",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.30.5",
    "fs-extra": "^11.2.0",
    "knip": "^5.82.0",
    "oxfmt": "^0.16.0",
    "oxlint": "^1.31.0",
    "oxlint-tsgolint": "^0.11.4",
    "p-map": "^7.0.3",
    "picocolors": "^1.1.1",
    "playwright": "^1.57.0",
    "react-native-pager-view": "^8.0.0",
    "rollup-plugin-visualizer": "^6.0.5",
    "sst": "3.17.10",
    "subset-font": "^2.4.0",
    "typescript": "^5.9.3",
    "valibot": "^1.1.0",
    "vite": "^7.1.12",
    "vitest": "^4.0.3",
    "xcodebuildmcp": "1.15.1"
  },
  "reanimated": {
    "staticFeatureFlags": {
      "ENABLE_SHARED_ELEMENT_TRANSITIONS": true,
      "ANDROID_SYNCHRONOUSLY_UPDATE_UI_PROPS": true,
      "IOS_SYNCHRONOUSLY_UPDATE_UI_PROPS": true,
      "USE_COMMIT_HOOK_ONLY_FOR_REACT_COMMITS": true,
      "DISABLE_COMMIT_PAUSING_MECHANISM": true,
      "FORCE_REACT_RENDER_FOR_SETTLED_ANIMATIONS": true
    }
  },
  "trustedDependencies": [
    "@rocicorp/zero-sqlite3"
  ],
  "env": {
    "AWS_ACCESS_KEY_ID": "",
    "AWS_SECRET_ACCESS_KEY": "",
    "APNS_ENDPOINT": "https://api.sandbox.push.apple.com",
    "APNS_TEAM_ID": "",
    "APNS_KEY_ID": "",
    "APNS_KEY": "",
    "BETTER_AUTH_SECRET": true,
    "BETTER_AUTH_URL": true,
    "CLOUDFLARE_R2_ACCESS_KEY": true,
    "CLOUDFLARE_R2_ENDPOINT": true,
    "CLOUDFLARE_R2_PUBLIC_URL": true,
    "CLOUDFLARE_R2_SECRET_KEY": true,
    "DEPLOYMENT_PLATFORM": "uncloud",
    "DEPLOYMENT_ARCH": "linux/amd64",
    "DEPLOY_HOST": "",
    "DEPLOY_USER": "root",
    "DEPLOY_SSH_KEY": "",
    "DEPLOY_DB": "",
    "ONE_SERVER_URL": true,
    "POSTMARK_SERVER_TOKEN": true,
    "VITE_DEMO_MODE": "",
    "VITE_POSTHOG_API_KEY": "",
    "VITE_POSTHOG_HOST": "",
    "VITE_ZERO_HOST": "",
    "VITE_WEB_HOST": "",
    "ZERO_NUM_SYNC_WORKERS": "2",
    "ZERO_CVR_MAX_CONNS": "4",
    "ZERO_UPSTREAM_MAX_CONNS": "8",
    "ZERO_PER_USER_MUTATION_LIMIT_MAX": "30",
    "ZERO_PER_USER_MUTATION_LIMIT_WINDOW_MS": "60000",
    "ZERO_MUTATE_FORWARD_COOKIES": "true",
    "ZERO_QUERY_FORWARD_COOKIES": "true",
    "ZERO_APP_PUBLICATIONS": "zero_takeout",
    "ZERO_VERSION": "$dep:@rocicorp/zero"
  },
  "publishConfig": {
    "access": "public"
  },
  "takeout": {
    "onboarded": false
  },
  "upgradeSets": {
    "takeout": [
      "@take-out/*",
      "on-zero"
    ],
    "tamagui": [
      "tamagui",
      "@tamagui/*"
    ],
    "one": [
      "one",
      "vxrn",
      "@vxrn/*"
    ],
    "zero": [
      "@rocicorp/zero"
    ],
    "better-auth": [
      "better-auth",
      "@better-auth/*"
    ]
  }
}
